{
  "name": "@agent-passport/openclaw-plugin",
  "version": "0.1.0",
  "description": "Agent Passport identity verification plugin for OpenClaw-style agent ecosystems",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./express": {
      "types": "./dist/express.d.ts",
      "import": "./dist/express.js"
    },
    "./fastify": {
      "types": "./dist/fastify.d.ts",
      "import": "./dist/fastify.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@zerobase-labs/passport-sdk": "^0.1.0"
  },
  "peerDependencies": {
    "express": ">=4.0.0",
    "fastify": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "express": { "optional": true },
    "fastify": { "optional": true }
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "fastify": "^4.26.0",
    "express": "^4.18.2",
    "typescript": "^5.4.0",
    "rimraf": "^5.0.0"
  },
  "keywords": [
    "agent-passport",
    "openclaw",
    "ai-agents",
    "identity",
    "authentication",
    "verification",
    "middleware",
    "ed25519",
    "jwt"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zerobase-labs/agent-passport",
    "directory": "examples/openclaw-plugin"
  }
